{
  "expo": {
    "name": "Bihevra",
    "slug": "tribunapplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "bihevra",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B111C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ee031be9-ba0f-407c-bd0f-9f8c65041939"
    },
    "runtimeVersion": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bihevra.tribun",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Taraftar anınızı fotoğrafla paylaşabilmeniz için kamera iznine ihtiyacımız var.",
        "NSPhotoLibraryUsageDescription": "Fotoğraf galerinizden seçim yapabilmeniz için izin gerekiyor.",
        "NSMicrophoneUsageDescription": "Sesli mesajlar için mikrofon iznine ihtiyacımız var.",
        "NSUserTrackingUsageDescription": "Size daha iyi deneyim sunmak ve içerikleri kişiselleştirmek için izninize ihtiyacımız var."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bihevra.tribun",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "NOTIFICATIONS"
      ],
      "edgeToEdgeEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-video",
      "expo-audio",
      "expo-tracking-transparency"
    ],
    "extra": {
      "eas": {
        "projectId": "ee031be9-ba0f-407c-bd0f-9f8c65041939"
      }
    }
  }
}
